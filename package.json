{
  "name": "project-catwalk",
  "version": "1.0.0",
  "description": "Front-End Capstone",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll --verbose",
    "test:coverage": "jest --verbose --coverage",
    "react-dev": "webpack --watch",
    "dev": "live-server ./client/dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rfp52-fec5-supersets/Project-Catwalk.git"
  },
  "author": "atvo912, mooseygoose, kwakster4, Alex-Garay",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rfp52-fec5-supersets/Project-Catwalk/issues"
  },
  "homepage": "https://github.com/rfp52-fec5-supersets/Project-Catwalk#readme",
  "dependencies": {
    "@babel/core": "^7.13.15",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.1",
    "axios": "^0.21.1",
    "babel-loader": "^8.2.2",
    "body-parser": "^1.19.0",
    "enzyme": "^3.11.0",
    "express": "^4.17.1",
    "jest": "^26.6.3",
    "jest-css-modules": "^2.1.0",
    "jest-enzyme": "^7.1.2",
    "moment": "^2.29.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-modal": "^3.13.1",
    "webpack": "^5.32.0"
  },
  "devDependencies": {
    "babel-jest": "^26.6.3",
    "chai": "^4.3.4",
    "enzyme-to-json": "^3.6.2",
    "sinon": "^10.0.0",
    "webpack-cli": "^4.6.0"
  },
  "jest": {
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "modulePaths": [
      "/shared/vendor/modules"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
